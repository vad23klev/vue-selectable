{
    "name": "vue-selectable",
    "version": "2.23.2",
    "description": "selectable component for vue",
    "license": "MIT",
    "main": "dist/vue-selectable.umd.min.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/vad23klev/vue-selectable.git"
    },
    "private": false,
    "scripts": {
        "build": "vue-cli-service build --name vue-selectable --entry ./vue-selectable.js --target lib",
        "lint": "vue-cli-service lint src example",
        "prepublishOnly": "npm run lint && npm run test:unit && npm run build:doc && npm run build"
    },
    "keywords": [
        "vue",
        "vuejs",
        "selectable",
        "list",
        "Selectable.js",
        "component",
        "nested"
    ],
    "dependencies": {
        "selectable.js": "^0.17.5"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.4.0",
        "@vue/cli-plugin-eslint": "^3.4.0",
        "@vue/cli-plugin-unit-jest": "^3.4.0",
        "@vue/cli-service": "^3.4.0",
        "@vue/eslint-config-prettier": "^4.0.1",
        "@vue/test-utils": "^1.0.0-beta.20",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "bootstrap": "^4.3.1",
        "codecov": "^3.2.0",
        "component-fixture": "^0.4.1",
        "element-ui": "^2.5.4",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "font-awesome": "^4.7.0",
        "jquery": "^3.3.1",
        "vue": "^2.6.8",
        "vue-cli-plugin-component": "^1.10.5",
        "vue-router": "^3.0.2",
        "vue-server-renderer": "^2.6.8",
        "vue-template-compiler": "^2.6.8",
        "vuetify": "^1.5.16",
        "vuex": "^3.1.1"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "files": [
        "dist/*.css",
        "dist/*.map",
        "dist/*.js",
        "src/*"
    ],
    "module": "dist/vue-selectable.common.js"
}